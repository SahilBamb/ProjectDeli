<% layout('/layouts/boilerplate') -%>
<h1>All Items</h1>
<% for (let item of items) {%>
    <item>
        <h3><%= item.name %></h3>
        <img src="<%= item.imgtrans %>">
        <p>Price: <%= item.price %></p>
        <p>Category: <%= item.category %></p>
        <a href="/items/<%= item._id %>">See Details</a>
    </item>
    <% } %>
